<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wolfhouse.springboot3initial.mvc.mapper.user.UserMapper">
    <update id="updateAccessedTimeByMap">
        <foreach collection="lastLogins" index="id" item="time">
            update user
            set login_date = #{time}
            where id = #{id};
        </foreach>
    </update>
</mapper>