<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wolfhouse.springboot3initial.mvc.mapper.auth.AuthenticationMapper">

    <select id="getChildIds" resultType="long">
        select id
        from authentication
        where parent_id in
        <foreach collection="authIds" open="(" close=")" separator="," item="id">
            #{id}
        </foreach>
    </select>
</mapper>
